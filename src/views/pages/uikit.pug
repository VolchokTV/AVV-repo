extends ../layouts/master.pug
//- Custom Configuration
block config
  - var adaptive = true // choose website type
  - var pageTitle = 'UI-kit'

block content
  main.page-main
    .container
      .header-uikit__logo
        <svg width="48" height="48" viewBox="0 0 48 48" fill="none" xmlns="http://www.w3.org/2000/svg">
